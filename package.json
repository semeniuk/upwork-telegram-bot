  "bugs": {
    "url": "https://github.com/semeniuk/upwork-telegram-bot/issues"
  },
  "homepage": "https://github.com/semeniuk/upwork-telegram-bot#readme",
  "dependencies": {
    "cross-env": "^5.2.0",
    "cross-var": "^1.1.0",
    "fastify": "^2.6.0",
    "request": "^2.88.0",
    "telegraf": "^3.30.1"
  }
}
